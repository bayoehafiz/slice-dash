<div class="row">
    <div class="col s12 m12 l12 center">
        <p class="flow-text" style="margin-top: 15%;">
            <form ng-submit="confirm(codes)" novalidate>
                <i class="material-icons large grey-text">phonelink_lock</i>
                <h3 class="grey-text">You are not authorized</h3>
                <h5 class="grey-text">Type your authorization codes here</h5>
                <br/>
                <input class="grey-text" id="name" type="phone" ng-model="codes" style="font-size:3.5rem;max-width: 35%;text-align: center;">
            </form>
        </p>
    </div>
</div>
